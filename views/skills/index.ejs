<%- include('../partials/header') %>
    <h1>Skills List</h1>
      <ul>
      <% skills.forEach(function(t) { %>
        <li>
          <a href="/skills/<%=t.id%>"> 
          <%= t.skill %>
            - 
          <%= t.learned ? 'learned' : 'not learned' %>
          </a>
        </li>
      <% }); %>
      <a href="/skills/new">Add a new skill</a>
    </ul>
</body>
</html>